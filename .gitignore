/db/schema.rb

# Ignore local
/local

/public/uploads
/public/.DS_Store
#ignore macfile
/.DS_Store
**/.DS_Store
**/*.swp

# dotenv
.env.development
.env.test
.env.production
.env

#simplecov
/coverage
/bfg.jar

# erd
**/erd.*
**/*.pdf

.vscode/
